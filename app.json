{
  "expo": {
    "name": "tetris",
    "slug": "Tetris",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./src/assets/imgSplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#006400"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rn.tetris"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/tetris.ttf"]
        }
      ]
    ]
  }
}
